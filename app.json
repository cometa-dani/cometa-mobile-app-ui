{
  "expo": {
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "name": "cometa",
    "slug": "cometa-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicons/android-icon-48x48.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/favicons/apple-icon-60x60.png",
      "backgroundColor": "#ffffff",
      "supportsTablet": true,
      "bundleIdentifier": "com.cometadaniexpo.cometaapp"
    },
    "android": {
      "icon": "./assets/favicons/android-icon-48x48.png",
      "backgroundColor": "#ffffff",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_cometa.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cometadaniexpo.cometaapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Regular.ttf",
            "./assets/fonts/Poppins-Bold.ttf",
            "./assets/fonts/Poppins-SemiBold.ttf",
            "./assets/fonts/Poppins-Medium.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Cometa accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow Cometa to access your microphone."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "66508126-cb4a-4207-8876-51455fbd4313"
      }
    },
    "owner": "cometa-dani-expo",
    "sdkVersion": "52.0.0"
  }
}
